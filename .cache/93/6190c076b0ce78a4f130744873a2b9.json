nerateMarkup(pokemon);\r\n    this._parentElement.innerHTML = \"\";\r\n    this._parentElement.insertAdjacentHTML(\"afterbegin\", markup);\r\n  }\r\n\r\n  _generateMarkup = function (pokemon) {\r\n    return [\r\n      `\r\n    <img\r\n    src=\"${pokemon.image}\"\r\n    alt=\"Charizard\"\r\n    class=\"pokemon__img--detail\"\r\n  />\r\n  <span class=\"id\">#${pokemon.id}</span>\r\n  <h2 class=\"name\">${pokemon.name}</h2>\r\n  <div class=\"types\">\r\n    <div class=\"type main__type type--${pokemon.maintype}\">${\r\n        pokemon.maintype\r\n      }</div>\r\n    <div class=\"type sub__type ${!pokemon.subtype ? \"hidden\" : \"\"} subtype--${\r\n        pokemon.subtype\r\n      }\">${pokemon.subtype}</div>\r\n  </div>\r\n  <div class=\"stats\">`,\r\n\r\n      pokemon.stats\r\n        .map((stat) => {\r\n          return `<div class=\"stat\">\r\n  <h4 class=\"type__name\">${stat.name}</h4>\r\n  <p class=\"stat__value\">${stat.stat}</p>\r\n  </div>\r\n  `;\r\n        })\r\n        .join(\"\"),\r\n      `\r\n  </div>\r\n  <div class=\"evolution\">\r\n    <h3>Evolution chain</h3>\r\n  \r\n    <div class=\"evolution__details\">`,\r\n      pokemon.evoImages\r\n        .map((img) => {\r\n          if (!img) {\r\n            return `<p>Does not evolve</p>`;\r\n          } else {\r\n            return `\r\n      <img\r\n      src=\"${img}\"\r\n      alt=\"Pokemon evolution\"\r\n      class=\"evo\"\r\n    />\r\n`;\r\n          }\r\n        })\r\n        .join(\"\"),\r\n      `</div>\r\n</div>`,\r\n    ].join(\"\");\r\n  };\r\n\r\n  addHandlerDetailsRender(handler) {\r\n    this._container.addEventListener(\"click\", function (e) {\r\n      const card = e.target.closest(\".card\");\r\n      const id = +card.dataset.id;\r\n\r\n      handler(id);\r\n    });\r\n\r\n    /*if (!this._element) {\r\n      window.onload = setTimeout(function () {\r\n        const _element = document.querySelector(\".card\");\r\n        _element.addEventListener(\"click\", function (e) {\r\n          console.log(\"Inside card click\");\r\n          const card = e.target.closest(\".card\");\r\n          const id = card.dataset.id;\r\n          handler(id);\r\n        });\r\n      }, 1000);\r\n    } else {\r\n      this._element.addEventListener(\"click\", function (e) {\r\n        console.log(\"Inside card click\");\r\n        const card = e.target.closest(\".card\");\r\n        const id = card.dataset.id;\r\n        handler(id);\r\n      });\r\n    }*/\r\n  }\r\n}\r\n\r\nexport default new PokeDetailsView();\r\n"},"lineCount":null}},"error":null,"hash":"1d1966c1c5311a7d882b2c7d13073ac1","cacheData":{"env":{}}}